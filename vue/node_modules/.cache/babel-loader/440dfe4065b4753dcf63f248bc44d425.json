{"ast":null,"code":"import { defineComponent } from 'vue';\nexport default defineComponent({\n  name: 'courseware-tab',\n  props: {\n    name: {\n      type: String,\n      required: true\n    },\n    alias: {\n      type: String,\n      default: ''\n    },\n    selected: {\n      type: Boolean,\n      default: false\n    },\n    index: {\n      type: Number,\n      required: true\n    },\n    icon: {\n      type: String,\n      default: ''\n    }\n  },\n\n  data() {\n    return {\n      isActive: false\n    };\n  },\n\n  computed: {\n    selectorId() {\n      return '#' + this._uid + '-' + this.name.toLowerCase().replace(/ /g, '-');\n    },\n\n    id() {\n      return this._uid + '-' + this.name.toLowerCase().replace(/ /g, '-') + '-tabpanel';\n    }\n\n  },\n\n  mounted() {\n    this.isActive = this.selected;\n  },\n\n  updated() {\n    if (this.isActive) {\n      window.STUDIP.eventBus.emit('courseware:update-tab', {\n        uid: this._uid\n      });\n    }\n  },\n\n  watch: {\n    selected(newValue) {\n      this.isActive = newValue;\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAcA,SAASA,eAAT,QAAgC,KAAhC;AACA,eAAeA,eAAe,CAAC;AAC7BC,MAAI,EAAE,gBADuB;AAE7BC,OAAK,EAAE;AACLD,QAAI,EAAE;AAAEE,UAAI,EAAEC,MAAR;AAAgBC,cAAQ,EAAE;AAA1B,KADD;AAELC,SAAK,EAAE;AAAEH,UAAI,EAAEC,MAAR;AAAgBG,aAAO,EAAE;AAAzB,KAFF;AAGLC,YAAQ,EAAE;AAAEL,UAAI,EAAEM,OAAR;AAAiBF,aAAO,EAAE;AAA1B,KAHL;AAILG,SAAK,EAAE;AAAEP,UAAI,EAAEQ,MAAR;AAAgBN,cAAQ,EAAE;AAA1B,KAJF;AAKLO,QAAI,EAAE;AAAET,UAAI,EAAEC,MAAR;AAAgBG,aAAO,EAAE;AAAzB;AALD,GAFsB;;AAS7BM,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE;AADL,KAAP;AAGD,GAb4B;;AAc7BC,UAAQ,EAAE;AACRC,cAAU,GAAG;AACX,aAAO,MAAM,KAAKC,IAAX,GAAkB,GAAlB,GAAwB,KAAKhB,IAAL,CAAUiB,WAAV,GAAwBC,OAAxB,CAAgC,IAAhC,EAAsC,GAAtC,CAA/B;AACD,KAHO;;AAIRC,MAAE,GAAG;AACH,aACE,KAAKH,IAAL,GACA,GADA,GAEA,KAAKhB,IAAL,CAAUiB,WAAV,GAAwBC,OAAxB,CAAgC,IAAhC,EAAsC,GAAtC,CAFA,GAGA,WAJF;AAMD;;AAXO,GAdmB;;AA2B7BE,SAAO,GAAG;AACR,SAAKP,QAAL,GAAgB,KAAKN,QAArB;AACD,GA7B4B;;AA8B7Bc,SAAO,GAAG;AACR,QAAI,KAAKR,QAA